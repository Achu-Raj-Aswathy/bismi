																<!-- Table body -->
																<tbody class="border-top-0">
																	<tr>
																		<td>0 hours to 24 hours</td>
																		<td>Non Refundable</td>
																	</tr>
																	<tr>
																		<td>24 hours to 365 days</td>
																		<td>$16,325 + $250</td>
